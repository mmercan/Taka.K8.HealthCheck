{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "UseNamespaceWatcher": false,
    "UseNamespaceScheduler": true,
    "SendEmail": true,
    "AddEvent": true,
    "AddEventonSuccess": true,
    "FeatureC": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "Beta": {
      "EnabledFor": [
        {
          "Name": "Headers",
          "Parameters": {
            "RequiredHeaders": [
              "Internal"
            ]
          }
        }
      ]
    }
  }
}